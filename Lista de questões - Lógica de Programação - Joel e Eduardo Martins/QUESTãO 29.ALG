Algoritmo "Questão 29"
// 0% de I.A. ela não vai fazer a prova por mim

Var
   x, ciclo, soma : inteiro
   media : real


Inicio
   //Não sei o que comentar, o código está simples
   para x<-13 ate 73 faca
      soma <- soma + x
      ciclo <- ciclo + 1
   fimpara
   media <- soma/ciclo
   escreval("A média aritmética dos números lidos entre 13 e 73 é", media)


Fimalgoritmo